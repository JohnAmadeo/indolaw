                  ]
                },
                {
                  "type": "LIST_ITEM",
                  "id": "",
                  "children": [
                    {
                      "type": "LETTER_WITH_DOT",
                      "text": "e."
                    },
                    {
                      "type": "PLAINTEXT",
                      "text": "bahwa berdasarkan pertimbangan sebagaimana dimaksud dalam huruf a, huruf b, huruf c, dan huruf d, perlu membentuk Undang-Undang tentang Keterbukaan Informasi Publik."
                    }
                  ]
