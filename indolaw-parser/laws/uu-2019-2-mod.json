                                  },
                                  {
                                    "type": "LIST_ITEM",
                                    "id": "",
                                    "children": [
                                      {
                                        "type": "NUMBER_WITH_RIGHT_BRACKET",
                                        "text": "5)"
                                      },
                                      {
                                        "type": "PLAINTEXT",
                                        "text": "komando, kendali, komunikasi, dan sistem informasi;"
                                      }
                                    ]
                                  },
                                  {
                                    "type": "LIST_ITEM",
                                    "id": "",
                                    "children": [
                                      {
                                        "type": "NUMBER_WITH_RIGHT_BRACKET",
                                        "text": "6)"
                                      },
                                      {
                                        "type": "PLAINTEXT",
                                        "text": "pendidikan, pelatihan, dan latihan; dan"
